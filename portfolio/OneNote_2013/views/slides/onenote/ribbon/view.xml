<root
    xmlns="http://certiport.com/hammer/2.x/slide" type="slide" id="ribbon_view">
    <view image="#images/ribbon/ribbon_view.jpg" width="958" height="118">
        <button x="49" y="29" class="hotspot ribbon-button" width="50" height="68" id="full_page_view_btn" tooltip="full_page_view">
            <events click="not_implemented"></events>
        </button>
        <button x="5" y="29" class="hotspot" width="44" height="68" id="normal_view_btn" tooltip="normal_view">
            <events click="not_implemented"></events>
        </button>
        <button x="103" y="29" class="hotspot ribbon-button" width="46" height="68" id="dock_to_desktop_btn" tooltip="dock_to_desktop">
            <events click="not_implemented"></events>
        </button>
        <button x="158" y="29" class="hotspot ribbon-button" width="42" height="68" id="page_color_btn" tooltip="page_color" toggle="true" value="page_color" model="rule">
            <events click="show_page_color"></events>
        </button>
        <button x="203" y="29" class="hotspot ribbon-button" width="40" height="35" id="rule_lines_top_btn" tooltip="rule_lines" toggle="true" value="rule_lines" model="rule">
            <events click="onenote.show_college_rule"></events>
        </button>
        <button x="203" y="64" class="hotspot ribbon-button" width="40" height="33" id="rule_lines_bottom_btn" tooltip="rule_lines2" toggle="true" value="rule_lines" model="rule">
            <events click="show_rule_lines"></events>
        </button>
        <button x="245" y="29" class="hotspot ribbon-button" width="56" height="68" id="hide_page_title_btn" tooltip="hide_page_title">
            <events click="not_implemented"></events>
        </button>
        <button x="313" y="29" class="hotspot ribbon-button" width="36" height="68" id="paper_size_btn" tooltip="paper_size">
            <events click="not_implemented"></events>
        </button>
        <button x="363" y="29" class="hotspot ribbon-button" width="36" height="68" id="zoom_out_btn" tooltip="zoom_out">
            <events click="not_implemented"></events>
        </button>
        <button x="406" y="29" class="hotspot ribbon-button" width="36" height="68" id="zoom_in_btn" tooltip="zoom_in">
            <events click="not_implemented"></events>
        </button>
        <button x="447" y="52" class="hotspot ribbon-button" width="50" height="20" id="100_btn" tooltip="100">
            <events click="not_implemented"></events>
        </button>
        <button x="447" y="75" class="hotspot ribbon-button" width="84" height="20" id="page_width_btn" tooltip="page_width">
            <events click="not_implemented"></events>
        </button>
        <button x="543" y="29" class="hotspot ribbon-button" width="48" height="68" id="new_window_btn" tooltip="new_window">
            <events click="not_implemented"></events>
        </button>
        <button x="593" y="29" class="hotspot ribbon-button" width="70" height="68" id="new_docked_window_btn" tooltip="new_docked_window">
            <events click="actions_recorded"></events>
        </button>
        <button x="666" y="29" class="hotspot ribbon-button" width="78" height="68" id="onenote_tool_btn" tooltip="send_to_onenote">
            <events click="not_implemented"></events>
        </button>
        <button x="746" y="29" class="hotspot ribbon-button" width="40" height="68" id="always_on_top_btn" tooltip="always_on_top">
            <events click="not_implemented"></events>
        </button>
    </view>
