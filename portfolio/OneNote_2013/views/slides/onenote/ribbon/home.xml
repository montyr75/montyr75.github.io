    <commands>
        <!--<command name="show_tags_menu">-->
        <!--<menu src="#views/slides/onenote/menus/tags_menu.xml" modal="true" group="menu" scroll="true" depth="1" direction="bottom"></menu>-->
        <!--</command>-->
        <command name="show_tags_menu">
            <menu src="#views/slides/onenote/menus/tags_menu.xml" modal="true" class="scroll" group="menu" depth="1" left="501" top="57"></menu>
            <condition>
                <if property="question.item" eq="B352">
                    <model target="tags_menu">
                        <symbol>{question.symbol}</symbol>
                        <display_name>{question.display_name}</display_name>
                        <label_color>{question.label_color}</label_color>
                    </model>
                </if>
            </condition>

        </command>
        <command name="show_font_size_menu">
            <menu src="#views/slides/onenote/menus/font_size_menu.xml" modal="true" group="menu" depth="1" left="248" top="87"></menu>
        </command>
