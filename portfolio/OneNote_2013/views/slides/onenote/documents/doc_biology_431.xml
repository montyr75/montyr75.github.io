<root
        xmlns="http://certiport.com/hammer/2.x/slide" type="slide" id="doc_biology">
    <events show="set_style"/>
    <view image="#images/documents/doc_biology_431.jpg" width="748" height="406">
        <text x="49" y="18" width="202" height="30" model="review_text" focus="start" lockFocus="start" class-model="question.style">
            <events rclick="show_text_rc_menu"></events>
        </text>
    </view>
    <model persist="true">
        <review_text>Exam review</review_text>
        <style>style-page-title</style>
    </model>
    <commands>
        <command name="set_style">
            <model target="question">
                <!--<style>style-page-title</style>-->
            </model>
        </command>
    </commands>
</root>