<root
    xmlns="http://certiport.com/hammer/2.x/slide" type="slide" id="doc_no_notebook">
    <view image="#images/documents/doc_no_notebook.jpg" width="748" height="408">
        <button x="140" y="150" class="hotspot" width="414" height="19">
            <events click="show_backstage_new"/>
        </button>
    </view>
    <model persist="true">
        <no_notebook>false</no_notebook>
    </model>
    <commands>
        <command name="show_backstage_new">
            <model>
                <no_notebook>true</no_notebook>
            </model>
            <exec command="onenote.show_backstage"/>
        </command>
    </commands>
</root>