<root
    xmlns="http://certiport.com/hammer/2.x/slide" type="slide" id="doc_biology">
    <view image="#images/documents/doc_biology_selected.jpg" width="748" height="408">
        <button x="0" y="0" class="hotspot" width="733" height="393">
            <events click="show_unselected" rclick="show_rc_menu"></events>
        </button>
    </view>
    <model persist="true"></model>
    <commands>
        <command name="show_unselected">
            <goto target="doc" src="#views/slides/onenote/documents/doc_biology.xml"/>
        </command>
        <command name="show_rc_menu">
            <menu src="#views/slides/onenote/menus/link_rc_menu.xml" modal="false" group="menu" depth="1"/>
        </command>
    </commands>
</root>