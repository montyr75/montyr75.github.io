    <commands>
        <command name="set_aqua_5mm">
            <model target="pen">
                <thickness>5</thickness>
            </model>
            <model target="color_thickness_dialog">
                <color>aqua</color>
                <marker>highlight</marker>
            </model>
            <model target="question">
                <thickness>5</thickness>
                <color>aqua</color>
                <tool>highlighter</tool>
            </model>
        </command>
        <command name="not_working">
            <model target="question">
                <thickness></thickness>
                <color></color>
                <tool>pen</tool>
            </model>
            <model target="pen">
                <thickness>2</thickness>
            </model>
            <model target="color_thickness_dialog">
                <color>black</color>
                <marker>pen</marker>
            </model>
            <exec command="not_implemented"></exec>
        </command>
