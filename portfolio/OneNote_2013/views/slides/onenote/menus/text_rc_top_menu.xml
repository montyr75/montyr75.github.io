        <tooltip name="underline" class="ui-tooltip">
            <html>
                Underline
            </html>
        </tooltip>
        <tooltip name="highlight" class="ui-tooltip">
            <html>
                Text Highlight Color
            </html>
        </tooltip>
        <tooltip name="font_color" class="ui-tooltip">
            <html>
                Font Color
            </html>
        </tooltip>
        <tooltip name="bullets" class="ui-tooltip">
            <html>
                Bullets
            </html>
        </tooltip>
        <tooltip name="numbering" class="ui-tooltip">
