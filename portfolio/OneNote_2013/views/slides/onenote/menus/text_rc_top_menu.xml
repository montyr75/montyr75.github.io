    <commands>
        <command name="show_styles_menu">
            <menu src="#views/slides/onenote/menus/text_rc_top_menu.xml" modal="false" group="menu" depth="1"/>
            <menu src="#views/slides/onenote/menus/styles_menu.xml" modal="true" group="menu" depth="1" top="350" ></menu>
        </command>
        <command name="show_tags_menu">
            <menu src="#views/slides/onenote/menus/tags_menu.xml" modal="true" class="scroll" group="menu" depth="1"></menu>
        </command>
    </commands>
