<root
    xmlns="http://certiport.com/hammer/2.x/slide" type="slide">
    <view image="#images/menus/taskpane_close_menu.jpg" width="103" height="72">
        <button x="2" y="1" class="hotspot ribbon-button" width="100" height="23" id="move_btn">
            <events click="not_implemented"></events>
        </button>
        <button x="2" y="24" class="hotspot ribbon-button" width="100" height="23" id="size_btn">
            <events click="not_implemented"></events>
        </button>
        <button x="2" y="47" class="hotspot ribbon-button" width="100" height="23" id="close_btn">
            <events click="onenote.close_panel"></events>
        </button>
    </view>
    <model persist="true"></model>
    <commands>
        <command name="close_taskpane">
            <goto target="doc" src="#views/slides/onenote/documents/doc_math.xml"/>
            <close target="*"/>
        </command>
    </commands>
</root>