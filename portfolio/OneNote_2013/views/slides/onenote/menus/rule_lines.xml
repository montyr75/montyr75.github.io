<root
    xmlns="http://certiport.com/hammer/2.x/slide" type="slide" id="rule_menu">
    <view image="#images/menus/rule_lines.jpg" width="284" height="212 ">
        <button x="58" y="25" class="hotspot rule-lines-button" width="47" height="46" id="narrow_btn" tooltip="narrow" toggle="true" model="question.rule" value="narrow">
            <events click="onenote.show_narrow_rule"></events>
        </button>
        <button x="2" y="25" class="hotspot rule-lines-button" width="46" height="46" id="none_btn" tooltip="none" toggle="true" model="question.rule" value="none">
            <events click="onenote.show_no_rule"></events>
        </button>
        <button x="2" y="104" class="hotspot rule-lines-button" width="46" height="46" id="small_btn" tooltip="small" toggle="true" model="question.rule" value="small">
            <events click="onenote.show_small_grid"></events>
        </button>
        <button x="58" y="104" class="hotspot rule-lines-button" width="46" height="46" id="medium_btn" tooltip="medium" toggle="true" model="question.rule" value="medium">
            <events click="onenote.show_medium_grid"></events>
        </button>
        <button x="114" y="104" class="hotspot rule-lines-button" width="46" height="46" id="large_btn" tooltip="large" toggle="true" model="question.rule" value="large">
            <events click="onenote.show_large_grid"></events>
        </button>
        <button x="170" y="104" class="hotspot rule-lines-button" width="46" height="46" id="very_large_btn" tooltip="very_large" toggle="true" model="question.rule" value="very_large">
            <events click="onenote.show_very_large_grid"></events>
        </button>
        <button x="3" y="185" class="hotspot ribbon-button" width="279" height="24" id="always">
            <events click="not_implemented"></events>
        </button>
        <button x="114" y="25" class="hotspot rule-lines-button" width="46" height="47" id="college_btn" tooltip="college" toggle="true" model="question.rule" value="college">
            <events click="onenote.show_college_rule"></events>
        </button>
        <button x="170" y="25" class="hotspot rule-lines-button" width="47" height="46" id="standard_btn" tooltip="standard" toggle="true" model="question.rule" value="standard">
            <events click="onenote.show_standard_rule"></events>
        </button>
        <button x="226" y="25" class="hotspot rule-lines-button" width="46" height="46" id="wide_btn" tooltip="wide" toggle="true" model="question.rule" value="wide">
            <events click="onenote.show_wide_rule"></events>
        </button>
    </view>
    <tooltips>
        <tooltip name="none" class="ui-ribbon-tooltip ribbon-tooltip-offset2">
            <html>
                <![CDATA[<img src="#images/tooltips/rule_lines/none_tt.jpg" width="39" height="25" >]]>
            </html>
        </tooltip>
        <tooltip name="narrow" class="ui-ribbon-tooltip ribbon-tooltip-offset2">
            <html>
                <![CDATA[<img src="#images/tooltips/rule_lines/narrow_tt.jpg" width="82" height="25" >]]>
            </html>
        </tooltip>
        <tooltip name="college" class="ui-ribbon-tooltip ribbon-tooltip-offset2">
            <html>
                <![CDATA[<img src="#images/tooltips/rule_lines/college_tt.jpg" width="83" height="25" >]]>
            </html>
        </tooltip>
        <tooltip name="standard" class="ui-ribbon-tooltip ribbon-tooltip-offset2">
            <html>
                <![CDATA[<img src="#images/tooltips/rule_lines/standard_tt.jpg" width="90" height="25" >]]>
            </html>
        </tooltip>
        <tooltip name="wide" class="ui-ribbon-tooltip ribbon-tooltip-offset2">
            <html>
                <![CDATA[<img src="#images/tooltips/rule_lines/wide_tt.jpg" width="70" height="25" >]]>
            </html>
        </tooltip>
        <tooltip name="small" class="ui-ribbon-tooltip ribbon-tooltip-offset2">
            <html>
                <![CDATA[<img src="#images/tooltips/rule_lines/small_tt.jpg" width="64" height="25" >]]>
            </html>
        </tooltip>
        <tooltip name="medium" class="ui-ribbon-tooltip ribbon-tooltip-offset2">
            <html>
                <![CDATA[<img src="#images/tooltips/rule_lines/medium_tt.jpg" width="80" height="25" >]]>
            </html>
        </tooltip>
        <tooltip name="large" class="ui-ribbon-tooltip ribbon-tooltip-offset2">
            <html>
                <![CDATA[<img src="#images/tooltips/rule_lines/large_tt.jpg" width="64" height="25" >]]>
            </html>
        </tooltip>
        <tooltip name="very_large" class="ui-ribbon-tooltip ribbon-tooltip-offset2">
            <html>
                <![CDATA[<img src="#images/tooltips/rule_lines/very_large_tt.jpg" width="90" height="25" >]]>
            </html>
        </tooltip>
    </tooltips>
    <model persist="true"></model>
    <commands></commands>
</root>